{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES5",
    "jsx": "react",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@pages/*": ["./src/pages/*"],
      "@routes/*": ["./src/routes/*"],
      "@utils/*": ["./src/utils/*"],
      "@store/*": ["./src/store/*"],
      "@public/*": ["./public/*"]
    }
  },

  "exclude": ["node_modules", "dist"]
}
